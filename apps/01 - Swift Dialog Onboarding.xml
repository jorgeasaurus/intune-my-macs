<MacIntuneManifest>
  <Type>Package</Type>
  <Name>01 - Onboarding Splashscreen</Name>
  <Description>Installs Swift Dialog onboarding splash screen and triggers core app installs.</Description>
  <Platform>macOS</Platform>
  <Category>Config</Category>
  <SourceFile>apps/01 - Swift Dialog Onboarding.pkg</SourceFile>
  <Package>
    <PrimaryBundleId>com.intune.swiftdialog</PrimaryBundleId>
    <PrimaryBundleVersion>1.3</PrimaryBundleVersion>
    <Publisher>Microsoft</Publisher>
    <MinimumSupportedOperatingSystem>v13_0</MinimumSupportedOperatingSystem>
    <IgnoreVersionDetection>true</IgnoreVersionDetection>
  <PreInstallScript>apps/01 - Swift Dialog Onboarding_pre.sh</PreInstallScript>
  <PostInstallScript>apps/01 - Swift Dialog Onboarding_post.sh</PostInstallScript>
  </Package>
</MacIntuneManifest>
